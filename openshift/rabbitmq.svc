apiVersion: v1
kind: Service
metadata:
  name: rabbitmq
spec:
  ports:
  - name: 15672-tcp
    port: 15672
    protocol: TCP
    targetPort: 15672
  - name: 5672-tcp
    port: 5672
    protocol: TCP
    targetPort: 5672
  selector:
    app: rabbitmq
    deploymentconfig: rabbitmq
status:
  loadBalancer: {}
